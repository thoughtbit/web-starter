{
  "name": "vue-vite-admin",
  "description": "Admin Pro for Vue",
  "private": true,
  "packageManager": "pnpm@6.32.3",
  "version": "1.0.0",
  "author": "moocss <moocss@gmail.com>",
  "scripts": {
    "serve": "pnpm dev",
    "dev:mock": "vite --open --mode mock",
    "dev": "vite --open",
    "build:staging": "vue-tsc --noEmit && vite build --mode staging",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --open",
    "report": "cross-env REPORT=true pnpm build",
    "prettier": "npx prettier --write .",
    "lint": "eslint ./src --ext .vue,.js,.ts,.tsx",
    "lint-fix": "eslint --fix ./src --ext .vue,.js,.ts,.tsx",
    "lint-staged": "pnpx lint-staged",
    "coverage": "vitest run --coverage",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "i": "pnpm install"
  },
  "dependencies": {
    "@antv/g6": "^4.6.4",
    "@element-plus/icons-vue": "^1.1.4",
    "@vueuse/core": "^8.7.5",
    "@vueuse/integrations": "8.7.5",
    "axios": "^0.26.1",
    "dayjs": "^1.11.0",
    "echarts": "^5.3.1",
    "element-plus": "^2.1.8",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.12",
    "pinia-plugin-persistedstate": "^1.0.3",
    "store2": "^2.13.2",
    "toastify-js": "^1.11.2",
    "vite-plugin-pwa": "^0.11.13",
    "vue": "^3.2.31",
    "vue-echarts": "^6.0.2",
    "vue-i18n": "^9.1.9",
    "vue-request": "^1.2.4",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^12.0.1",
    "@types/echarts": "^4.9.12",
    "@types/lodash-es": "^4.17.6",
    "@types/nanoid": "^3.0.0",
    "@types/node": "^17.0.23",
    "@types/nprogress": "^0.2.0",
    "@types/toastify-js": "^1.11.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vitejs/plugin-legacy": "^1.7.1",
    "@vitejs/plugin-vue": "^2.2.0",
    "@vitejs/plugin-vue-jsx": "^1.3.8",
    "@vitest/ui": "^0.8.4",
    "@vue/test-utils": "^2.0.0-rc.17",
    "@vueuse/core": "^8.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "happy-dom": "^2.50.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "mockjs": "^1.1.0",
    "nanoid": "^3.3.1",
    "pnpm": "^6.32.4",
    "prettier": "^2.6.0",
    "rollup-plugin-visualizer": "^5.6.0",
    "sass": "^1.49.9",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "typescript": "^4.5.4",
    "unplugin-vue-components": "^0.18.5",
    "vite": "^2.9.13",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eslint": "^1.3.0",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-inspect": "^0.4.3",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-windicss": "^1.8.3",
    "vite-svg-loader": "^3.1.2",
    "vitest": "^0.8.4",
    "vue-tsc": "^0.33.9",
    "windicss": "^3.5.1",
    "windicss-analysis": "^0.3.5"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.vue": [
      "stylelint --fix",
      "prettier --write",
      "eslint --fix"
    ],
    "*.{less,css}": [
      "stylelint --fix",
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "resolutions": {
    "bin-wrapper": "npm:bin-wrapper-china",
    "rollup": "^2.56.3",
    "gifsicle": "5.2.0"
  }
}
