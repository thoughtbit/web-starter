{
  "compilerOptions": {
    "target": "es2017",
    "jsx": "preserve",
    "lib": ["esnext", "DOM"],
    "module": "esnext",
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "strict": true,
    "strictNullChecks": true,
    "esModuleInterop": true
  },
  "include": [
    "env.d.ts",
    "storybook/**/*.story.vue",
    "src/**/*",
    "src/**/*.vue"
  ]
}
