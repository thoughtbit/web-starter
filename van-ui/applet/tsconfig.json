{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "esnext",
    "lib": ["esnext", "dom"],
    "module": "esnext",
    "paths": {
      "~/*": ["./src/*"]
    },
    "types": [
      "vite/client"
    ],
    "outDir": "./dist",
    "skipLibCheck": true
  },
  "include": ["src/**/*.vue", "src/**/*.d.ts", "src/**/*.ts"]
}
