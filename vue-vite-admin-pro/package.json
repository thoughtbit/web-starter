{
  "name": "admin-app",
  "version": "0.2.0",
  "private": true,
  "author": "moocss <moocss@gmail.com>",
  "packageManager": "pnpm@6.32.3",
  "scripts": {
    "serve": "pnpm dev",
    "dev": "vite --open",
    "dev:mock": "vite --open --mode mock",
    "build": "vue-tsc --noEmit && vite build",
    "build:staging": "vue-tsc --noEmit && vite build --mode staging",
    "preview": "vite preview --open",
    "prettier": "npx prettier --write .",
    "lint": "eslint ./src --ext .vue,.js,.ts,.tsx",
    "lint-fix": "eslint --fix ./src --ext .vue,.js,.ts,.tsx",
    "lint-staged": "npx lint-staged",
    "coverage": "vitest --coverage",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "i": "pnpm install",
    "commit": "git-cz"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.vue": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{less,css}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@vueuse/core": "^10.7.2",
    "@wangeditor/editor": "^5.1.23",
    "async-validator": "^4.2.5",
    "axios": "^1.6.7",
    "dayjs": "^1.11.10",
    "echarts": "^5.5.0",
    "element-plus": "^2.5.6",
    "lodash": "^4.17.21",
    "mitt": "^3.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "qs": "^6.11.2",
    "store2": "^2.14.2",
    "toastify-js": "^1.12.0",
    "vue": "^3.4.19",
    "vue-content-loader": "^2.0.1",
    "vue-echarts": "^6.6.8",
    "vue-i18n": "^9.9.1",
    "vue-request": "^2.0.4",
    "vue-router": "^4.2.5",
    "xgplayer": "^3.0.12"
  },
  "devDependencies": {
    "@types/echarts": "^4.9.18",
    "@types/lodash": "^4.14.196",
    "@types/mockjs": "^1.0.7",
    "@types/nanoid": "^3.0.0",
    "@types/node": "^20.4.9",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7",
    "@types/toastify-js": "^1.11.0",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "@vitejs/plugin-legacy": "^4.1.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vitest/ui": "^1.3.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.1",
    "autoprefixer": "^10.4.14",
    "commitizen": "^4.3.0",
    "commitlint-config-cz": "^0.13.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.46.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.16.1",
    "husky": "^9.0.0",
    "less": "^4.1.2",
    "lint-staged": "^12.2.2",
    "mockjs": "^1.1.0",
    "nanoid": "^3.1.30",
    "postcss": "^8.4.27",
    "prettier": "^3.0.1",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.63.4",
    "typescript": "^5.1.3",
    "unocss": "^0.54.2",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^5.1.3",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-externals": "^0.6.2",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-mock": "^3.0.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-external": "^4.3.0",
    "vite-svg-loader": "^5.1.0",
    "vitest": "^1.3.0",
    "vue-tsc": "^1.8.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "resolutions": {
    "bin-wrapper": "npm:bin-wrapper-china"
  },
  "license": "MIT"
}
